    <text>节日祝福</text>
  </navigator>
  <navigator hover-class="none" url="/pages/weather/index/index">
    <image src="https://m.youzhu.com/images/weChatAppImages/icon_2.png" />
    <text>天气预报</text>
  </navigator>
  <navigator hover-class="none" url="/pages/music/index/index">
    <image src="https://m.youzhu.com/images/weChatAppImages/icon_3.png" />
    <text>音乐随身听</text>
  </navigator>
  <navigator hover-class="none" url="/pages/todolist/index/index">
    <image src="https://m.youzhu.com/images/weChatAppImages/icon_4.png" />
    <text>备忘录</text>
  </navigator>
  <navigator hover-class="none" url="/pages/calculator/index/index">
    <image src="https://m.youzhu.com/images/weChatAppImages/icon_5.png" />
    <text>计算器</text>
  </navigator>
  <navigator hover-class="none" url="/pages/qrcode/index/index">
    <image src="https://m.youzhu.com/images/weChatAppImages/icon_6.png" />
    <text>二维码生成器</text>
  </navigator>
</view>
<view class="ws_kf">
  <button open-type="contact">联系客服</button>
</view>
